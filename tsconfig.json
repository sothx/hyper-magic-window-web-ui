{
	"compilerOptions": {
		"strict": true,
		"jsx": "preserve",
		"target": "ESNext",
		"module": "ESNext",
		"esModuleInterop": true,
		"jsxImportSource": "vue",
		"verbatimModuleSyntax": true,
		"moduleResolution": "Bundler",
		"types": ["node"],
		"paths": {
			"@/*": ["./src/*"],
			"$/*": ["./node_modules/*"]
		}
	},
	"include": ["**/*.ts", "**/*.tsx","src/**/*.vue"],
	"exclude": ["node_modules", "dist"]
}
